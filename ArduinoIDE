// C++ code
//
int ledPin=12;
int ledPin2 = 8;
void setup()
{
  pinMode(ledPin, OUTPUT);
  pinMode(ledPin2,OUTPUT);
}

void loop()
{
  digitalWrite(ledPin, HIGH);
  digitalWrite(ledPin2, LOW);
  delay(700); // Wait for 1000 millisecond(s)
  digitalWrite(ledPin, LOW);
  digitalWrite(ledPin2, HIGH);
  delay(700); // Wait for 1000 millisecond(s)
}
